@inherits DotNetNuke.Web.Mvc.Framework.DnnWebViewPage<IEnumerable<Upendo.Modules.UpendoUserManaged.Models.DnnModel.UserRoles>>



<table class="table table-striped table-bordered">
    <thead>
        <tr style="background-color:#6d5a2c; color:#FFFFFF">
            <th class="hidden">Id</th>
            <th>NAME</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td class="hidden">
                    @item.UserId
                </td>
                <td>
                    @item.Role.RoleName
                </td>



            </tr>
        }
    </tbody>
</table>

<script>
$(document).ready(function () {

});
</script>